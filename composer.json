{
    "name": "tdev-group/laravel-sso-client",
    "description": "Laravel Sso Client provides identity server client support to laravel",
    "keywords": [
        "laravel",
        "oauth",
        "is4",
        "identityserver4",
        "identity-server4"
    ],
    "authors": [
        {
            "name": "Terro",
            "email": "wali7981@gmail.com"
        }
    ],
    "type": "library",
    "require": {
        "php": "^8.5",
        "firebase/php-jwt": "^7.0",
        "illuminate/http": "^12.53",
        "illuminate/auth": "^12.53",
        "illuminate/database": "^12.53",
        "illuminate/support": "^12.53",
        "illuminate/validation": "^12.53",
        "guzzlehttp/guzzle": "^7.10"
    },
    "autoload": {
        "psr-4": {
            "LaravelSsoClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelSsoClient\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelSsoClient\\Providers\\SsoServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^13.0",
        "mockery/mockery": "^1.5",
        "orchestra/testbench": "^10.9"
    }
}
